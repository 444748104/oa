{
  "name": "steedos-platform",
  "private": true,
  "workspaces": [
    "packages/*",
    "services/*",
    "ee/*",
    "server",
    "examples/*",
    "steedos-packages/*"
  ],
  "scripts": {
    "start": "cd examples && cd project-template && yarn start",
    "start:ee": "cd examples && cd project-ee && yarn start",
    "bootstrap": "lerna clean --yes && lerna bootstrap --force-local",
    "build": "lerna run build",
    "i18n": "lerna run i18n"
  },
  "resolutions": {
    "**/typescript": "4.6.3",
    "**/pass-stream": "1.0.0"
  },
  "devDependencies": {
    "lerna": "^5.1.6"
  },
  "peerDependencies": {
    "steedos-server": "2.x"
  },
  "dependencies": {}
}
