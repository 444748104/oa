{
  "name": "steedos-platform",
  "private": true,
  "workspaces": [
    "packages/*",
    "services/*",
    "ee/*",
    "server",
    "examples/*",
    "steedos-packages/*"
  ],
  "scripts": {
    "start": "cd examples && cd project-template && yarn start",
    "start:ee": "cd examples && cd project-ee && yarn start",
    "bootstrap": "lerna clean --yes && lerna bootstrap --force-local",
    "build": "lerna run build",
    "i18n": "lerna run i18n"
  },
  "resolutions": {
    "**/cryptiles": "^4.1.2",
    "**/typescript": "4.6.3",
    "**/pass-stream": "1.0.0"
  },
  "devDependencies": {
    "@octokit/core": "^3.5.1",
    "cpy": "7.3.0",
    "lerna": "4.0.0",
    "mkdirp": "^1.0.4",
    "node-stream-zip": "1.15.0"
  },
  "peerDependencies": {
    "steedos-server": "2.x"
  },
  "dependencies": {}
}
