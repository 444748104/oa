{
  "name": "steedos-platform",
  "private": true,
  "workspaces": [
    "packages/*",
    "services/*",
    "ee/*",
    "server",
    "examples/*",
    "steedos-packages/*"
  ],
  "scripts": {
    "start": "cd examples && cd project-template && yarn start",
    "start:ee": "cd examples && cd project-ee && yarn start",
    "bootstrap": "lerna clean --yes && lerna bootstrap --force-local",
    "build": "lerna run build",
    "i18n": "lerna run i18n"
  },
  "resolutions": {
  },
  "devDependencies": {
    "lerna": "^4.0.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.2"
  },
  "peerDependencies": {
    "steedos-server": "2.x"
  },
  "dependencies": {}
}
