{
  "name": "project-ee",
  "version": "2.3.1-beta.1",
  "private": true,
  "scripts": {
    "start": "yarn start:objectServer && yarn start:metadata && yarn start:core",
    "repl": "moleculer-runner --repl -c moleculer-repl.config.js",
    "start:metadata": "export DEBUG=steedos:metadata:* && steedos start",
    "start:core": "export DEBUG=steedos:metadata:* && moleculer-runner platformService.service.js -c moleculer-core.config.js",
    "start:objectServer": "export DEBUG=steedos:metadata:* && moleculer-runner objectServiceFactory.service.js",
    "debug": "export DEBUG=steedos:metadata:* && steedos start"
  },
  "dependencies": {
    "@steedos-labs/experience": "^1.0.0-beta.20",
    "@steedos/ee_unpkg-local": "2.3.1-beta.1",
    "@steedos/service-enterprise": "2.3.1-beta.1"
  }
}
