                                "requestAdaptor": "\n        var filters = '[]';\n        var top = 10;\n        if(api.data.$term){\n            filters = '[\"name\", \"contains\", \"'+ api.data.$term +'\"]';\n        }else if(api.data.$value){\n            filters = '[\"_id\", \"=\", \"'+ api.data.$value +'\"]';\n        }\n        api.data.query = api.data.query.replace(/{__filters}/g, filters).replace('{__top}', top);\n        return api;\n    ",
                                "adaptor": "\n        payload.data.options = eval(function anonymous(\n) {\n\n  var profiles = Creator.odata.query('permission_set', {$filter: \"(type eq 'profile')\"}, true);\n  var result = [];\n  profiles.forEach(function (item) {\n    result.push({\n      label: item.label,\n      value: item.name\n    });\n  });\n  return result;\n\n})(api.data);\n        return payload;\n        "
