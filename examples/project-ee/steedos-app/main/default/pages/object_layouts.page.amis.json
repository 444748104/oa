                        },
                        {
                            "name": "related_lists",
                            "label": "相关子表",
                            "type": "input-table",
                            "strictMode": false,
                            "affixHeader": false,
                            "editable": true,
                            "addable": true,
                            "removable": true,
                            "draggable": true,
                            "columns": [
                                {
                                    "name": "related_field_fullname",
                                    "label": "子表名称",
                                    "quickEdit": {
                                        "type": "select",
                                        "joinValues": false,
